<Window x:Class="ariketa1_U1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ariketa1_U1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="500">
    <Grid>
        <Button x:Name="ftpBtn" Content="Lanzar proceso FTP" HorizontalAlignment="Left" Margin="102,75,0,0" VerticalAlignment="Top" Height="100" Width="150" Click="ftpBtn_Click"/>
        <Button x:Name="killProcessBtn" Content="Matar Proceso" HorizontalAlignment="Left" Margin="281,75,0,0" VerticalAlignment="Top" Height="100" Width="150" Click="KillProcessBtn_Click"/>
        <Button x:Name="showProcess" Content="Mostrar procesos" HorizontalAlignment="Left" Margin="102,217,0,0" VerticalAlignment="Top"  Height="100" Width="150" Click="ShowProcess_Click"/>
        <ComboBox x:Name="comboProcess" HorizontalAlignment="Left" Margin="281,256,0,0" VerticalAlignment="Top" Width="150"/>
        <Label x:Name="lblProcessId" Content="" HorizontalAlignment="Left" Margin="281,192,0,0" VerticalAlignment="Top" Width="150" Height="25"/>

    </Grid>
</Window>
